<script type='text/javascript'>
    //dom载入完毕执行
    jQuery(function()
    {
        //幻灯片开启
        $('.bxslider').bxSlider({'mode':'fade','captions':false,'controls':true,'pager':false,'auto':true});


        //公告切换
        $(".tnoice_nav>li:eq(0)").hover(function(){
            $(this).addClass('curr_tnoice_tit').siblings().removeClass('curr_tnoice_tit');
            $('.tnoice_count:eq(0)').show();
            $(".tnoice_count:eq(1)").hide();
        });
        $(".tnoice_nav>li:eq(1)").hover(function(){
            $(this).addClass('curr_tnoice_tit').siblings().removeClass('curr_tnoice_tit');
            $('.tnoice_count:eq(1)').show();
            $(".tnoice_count:eq(0)").hide();
        });
        $(".tnoice_nav>li:eq(0)").trigger('mouseenter');



        $(".tpromotion_nav ul>li").hover(function(){
            $(this).addClass('select').siblings().removeClass('select');
            $('.tpromotion_count').eq($(".tpromotion_nav ul>li").index(this)).show().siblings(".tpromotion_count").hide();

            $('.tpromotion_count').children('ul').eq($('.tpromotion_nav ul>li').index(this)).find("img").each(function(){
                var img = this;
                if(img.getAttribute("initsrc")){        //alert(2);
                    img.src = img.getAttribute("initsrc");
                    img.removeAttribute("initsrc");
                }
            });
        });
        $(".tpromotion_nav ul>li:eq(0)").trigger('mouseenter');



        $(".tpage_count2 .tsc_pro_nav li").hover(function(){
            $(this).addClass('select').siblings().removeClass('select');
            $(this).parents('.tpage_count2').find('.tsc_pro_count').eq($(this).parents('.tpage_count2').find('.tsc_pro_nav li').index(this)).show().siblings(".tsc_pro_count").hide();
        });


        $(".tpro_top .tpro_top_count").hover(function(){
            $(this).siblings(".tpro_top_count").removeClass("tpro_tab");
            $(this).addClass("tpro_tab");

        });

        });

        
</script>
